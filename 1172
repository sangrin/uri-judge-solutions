import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner input = new Scanner(System.in);

		int vetor[] = new int[10];

		for (int i = 0; i < 10; i++) {
			int aux = input.nextInt();
			if (aux <= 0) {
				vetor[i] = 1;
			} else {
				vetor[i] = aux;
			}
			System.out.println("X[" + i + "] = " + vetor[i]);
		}
	}

}
